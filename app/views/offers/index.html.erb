<p id="notice"><%= notice %></p>

<h1>Offers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Ownerid</th>
      <th>Installs</th>
      <th>Revenue</th>
      <th>Risk</th>
      <th>Finalstatus</th>
      <th>Comment1</th>
      <th>Comment2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offers.each do |offer| %>
      <tr>
        <td><%= offer.name %></td>
        <td><%= offer.ownerID %></td>
        <td><%= offer.installs %></td>
        <td><%= offer.revenue %></td>
        <td><%= offer.risk %></td>
        <td><%= offer.finalStatus %></td>
        <td><%= offer.comment1 %></td>
        <td><%= offer.comment2 %></td>
        <td><%= link_to 'Show', offer %></td>
        <td><%= link_to 'Edit', edit_offer_path(offer) %></td>
        <td><%= link_to 'Destroy', offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offer', new_offer_path %>
